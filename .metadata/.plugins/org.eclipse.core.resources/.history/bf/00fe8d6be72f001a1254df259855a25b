#define PORTB_PIN2_INITIALVALUE	1
#define PORTB_PIN3_INITIALVALUE	1
#define PORTB_PIN4_INITIALVALUE	1
#define PORTB_PIN5_INITIALVALUE	1
#define PORTB_PIN6_INITIALVALUE	1
#define PORTB_PIN7_INITIALVALUE	1
#define PORTC_PIN0_INITIALVALUE	1//pullup resistor activated
#define PORTC_PIN1_INITIALVALUE	0//initial signal is low
#define PORTC_PIN2_INITIALVALUE	0
#define PORTC_PIN3_INITIALVALUE	0
#define PORTC_PIN4_INITIALVALUE	0
#define PORTC_PIN5_INITIALVALUE	0
