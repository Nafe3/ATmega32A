/*Stop bits*/
#define	UART_u8_STOP_BITS		UART_STOP_1_BIT

/*Options:	 UART_SYS_FREQ_8M
*            UART_SYS_FREQ_12M
*            UART_SYS_FREQ_16M
*/

#define UART_u8_SYS_FREQ





