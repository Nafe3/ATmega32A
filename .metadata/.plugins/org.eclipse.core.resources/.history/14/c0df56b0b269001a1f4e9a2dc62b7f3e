#include <util/delay.h>
#include"STD_TYPES.h"
#include"DIO_register.h"
int main(void)
{
DDRD=0xff;

while(1)
{
	u8 i;
	u8 j;
for(i=0;i<=255;i++)
{
	PORTD=i;
	_delay_ms(20);
}
for(j=255;j>=0;j--)
{
	PORTD=j;
	_delay_ms(100);
}
}
}

